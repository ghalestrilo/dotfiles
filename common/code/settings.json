{
  "window.menuBarVisibility": "toggle",
  "terminal.integrated.fontSize": 14,
  "workbench.statusBar.visible": false,
  "workbench.activityBar.visible": true,
  "editor.renderWhitespace": "boundary",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/node_modules": true,
    "node_modules": true,
    "venv": true,
    "*.sublime-*": true,
    "env*": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.o": true,
    "**/CVS": true
  },
  "colorize.languages": ["javascript", "typescript"],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "haskell.hlint.ignore": ["Redundant bracket", "Use fromMaybe"],
  "files.associations": {
    "*.tidal": "haskell",
    "*.krb": "python",
    "**/.i3/config": "i3",
    "**/i3/config": "i3"
  },
  "tidalcycles.bootTidalPath": "/Users/admin/git/libtidal/boot.tidal",
  "terminal.integrated.fontFamily": "monospace",
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": true,
  "editor.fontFamily": "Hasklig",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "terminal.integrated.rendererType": "dom",
  "python.dataScience.askForKernelRestart": false,
  "editor.detectIndentation": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "eslint.format.enable": true,
  "telemetry.enableTelemetry": false,
  /*
  "bracket-pair-colorizer-2.colors": [
    "#222233"
  ],
  "bracket-pair-colorizer-2.hightlightActiveScope": true
  */
  "sync.ignoredExtensions": [],
  "javascript.updateImportsOnFileMove.enabled": "never",
  "todohighlight.keywords": [
    {
      "text": "NOTE:",
      "color": "#000",
      "backgroundColor": "yellow",
      "overviewRulerColor": "grey"
    },
    {
      "text": "FIXME:",
      "color": "#000"
    },
    {
      "text": "IDEA:",
      "color": "#000",
      "backgroundColor": "cyan"
    },
    {
      "text": "TODO:", // TODO:
      "color": "#000",
      "backgroundColor": "yellow"
      //other styling properties goes here ...
    }
  ],
  "editor.formatOnSave": true,
  "zenMode.centerLayout": false,
  "flow.showUncovered": true,
  "flow.coverageSeverity": "warn",
  "terminal.external.linuxExec": "alacritty",
  "flow.useBundledFlow": false,
  "flow.pathToFlow": "",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "latex.server.autoDownload": true,
  "hediet.vscode-drawio.local-storage": "",
  "workbench.editor.untitled.hint": "hidden",
  "calva.paredit.defaultKeyMap": "original",
  "workbench.iconTheme": "material-icon-theme",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "elixir.credo.ignoreWarningMessages": true,
  "haskell.manageHLS": "GHCup",
  "emmet.includeLanguages": {
    "html-eex": "html",
    "elixir": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "elixirLS.fetchDeps": false,
  "editor.guides.indentation": false,
  "workbench.colorTheme": "Noctis",
  "tidalcycles.useStackGhci": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "window.zoomLevel": -1
}
